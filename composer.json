{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/rattfieldnz/laravel-sitemap",
            "branch": "dev-master"
        }
    ],
    "require": {
        "laravel/framework": "5.3.*",
        "cviebrock/eloquent-sluggable": "3.1.4",
        "laravelcollective/html": "5.3.*",
        "guzzlehttp/guzzle": "^6.1",
        "michelf/php-markdown": "~1.5",
        "barryvdh/laravel-ide-helper": "~2.0",
        "doctrine/dbal": "~2.5",
        "way/generators": "~3.0",
        "rattfieldnz/url-validation": "dev-master",
        "roumen/sitemap": "dev-master",
        "abraham/twitteroauth": "^0.5.3",
        "roumen/feed": "^2.9",
        "chromabits/purifier": "^2.1",
        "laracasts/generators": "~1.0",
        "laravelcollective/bus": "^5.2.0",
        "league/csv": "^8.1",
        "michelecurletta/laravel-schedule-overview": "^0.0.2",
        "predis/predis": "1.1.*",
        "php": ">=5.6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "phpspec/phpspec": "~2.1",
        "socialengine/sniffer-rules": "^2.2",
        "phpmd/phpmd": "@stable",
        "symfony/dom-crawler": "3.1.*",
        "symfony/css-selector": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database",
            "app"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}